container_commands:
  01_symlink_node_executable:
    command: "ln -sf `ls -td /opt/elasticbeanstalk/node-install/node-v10.22.1* | head -1`/bin/node /usr/local/bin/node"
  02_symlink_npm_executable:
    command: "ln -sf `ls -td /opt/elasticbeanstalk/node-install/node-v10.22.1* | head -1`/bin/node /usr/local/bin/npm"
  03_symlink_phantomjs:
    command: "ln -sf /var/app/current/node_modules/phantomjs-prebuilt/bin/phantomjs /usr/local/bin/phantomjs"
option_settings:
  - option_name: 'NODE_ENV'
    value: 'production'
